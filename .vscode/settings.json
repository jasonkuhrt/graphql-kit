{
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "json.schemas": [
    {
      "fileMatch": [
        "package.json"
      ],
      "path": "./node_modules/libra/schema/package-json-libra.json"
    }
  ],
  "files.exclude": {
    "tsconfig.json": true,
    "tsconfig.build.json": true,
    "tsconfig.libra.json": true,
    "build": true,
    ".gitignore": true,
    ".github": true,
    "pnpm-workspace.yaml": true,
    ".vscode": true,
    ".zed": true,
    "dprint.json": true,
    "eslint.config.js": true,
    "vitest.config.ts": true,
    "node_modules": true,
    ".dprint": true,
    ".vitepress/dist": true,
    ".vitepress/cache": true
  }
}
